{
  "name": "@xgheaven/homepage",
  "version": "0.0.0",
  "description": "XGHeaven HomePage",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "parcel src/index.html",
    "build": "npm run build:html && npm run build:server && npm run build:dump && node ./dist/build.js ./dist/server.js",
    "build:html": "rm -rf ./dist/ && parcel build src/index.html --detailed-report --no-source-maps -d ./dist/html",
    "fmt": "prettier \"src/**/*.{ts,tsx}\" -w",
    "build:dump": "parcel build src/build.ts -t node -o ./dist/build.js",
    "build:server": "parcel build src/server.ts -t node --no-minify -o ./dist/server.js"
  },
  "author": "XGHeaven",
  "license": "MIT",
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^7.10.3",
    "@nestjs/platform-express": "^7.6.15",
    "apollo-server-express": "^2.23.0",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "color": "^3.1.3",
    "date-fns": "^2.21.0",
    "fs-extra": "^9.1.0",
    "github-markdown-css": "^4.0.0",
    "glob": "^7.1.6",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "graphql-type-json": "^0.3.2",
    "heap-js": "^2.1.5",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "mdast-util-frontmatter": "^0.2.0",
    "mdast-util-toc": "^5.1.0",
    "mobx": "^6.1.8",
    "mobx-react": "^7.1.0",
    "rc-tooltip": "^5.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "reflect-metadata": "^0.1.13",
    "remark-frontmatter": "^3.0.0",
    "remark-parse": "^9.0.0",
    "remark-slug": "^6.0.0",
    "rxjs": "^6.6.7",
    "unified": "^9.2.1",
    "unist-util-filter": "^2.0.3",
    "unist-util-map": "^2.0.0",
    "unist-util-remove": "^2.1.0",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@emotion/babel-plugin": "^11.2.0",
    "@types/color": "^3.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/glob": "^7.1.3",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash-es": "^4.17.4",
    "@types/rc-tooltip": "^3.7.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "babel-core": "^6.26.3",
